<h1>Client List</h1>


<table class="customers--table">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Gallery Web Address</th>
  </tr>
  <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.first_name %></td>
      <td><%= customer.last_name %></td>
      <td><%= customer.gallery_web_address %></td>
    </tr>
  <% end %>
</table>



<%= link_to "Add New Client", new_customer_path %>
